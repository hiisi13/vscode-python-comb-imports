{
    "name": "python-comb-imports",
    "displayName": "python-comb-imports",
    "description": "Reorder your imports",
    "version": "0.0.1",
    "publisher": "hiisi13",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "keybindings": [{
            "command": "extension.compPythonImports",
            "key": "ctrl+p ctrl+c",
            "mac": "cmd+p cmd+c",
            "when": "editorTextFocus"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}